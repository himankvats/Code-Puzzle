      <Button fx:id="loginButton" layoutX="103.0" layoutY="302.0" mnemonicParsing="false" onAction="#handleLoginButtonAction" text="Login" AnchorPane.leftAnchor="250.0" />
      <Button fx:id="exitButton" layoutX="416.0" layoutY="302.0" mnemonicParsing="false" onAction="#handleExitButtonAction" prefHeight="27.0" prefWidth="52.0" text="Exit" AnchorPane.rightAnchor="250.0" />
